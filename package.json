{
  "name": "bootue",
  "description": "Bootstrap + Vue 2 = 100% Components",
  "version": "0.1.14",
  "author": "Felipe Signorini <felipeklerk@yahoo.com.br>",
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev-build": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maestro-server/bootue.git"
  },
  "dependencies": {
    "vue": "^2.2.1",
    "node-sass": "^4.5.0"
  },
  "devDependencies": {
    "babel-core": "^6.0.0",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.0.0",
    "babel-preset-latest": "^6.0.0",
    "cross-env": "^3.0.0",
    "css-loader": "^0.25.0",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^6.2.1",
    "eslint-friendly-formatter": "^2.0.7",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-html": "^2.0.1",
    "eslint-plugin-promise": "^3.4.0",
    "eslint-plugin-standard": "^2.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "file-loader": "^0.9.0",
    "prismjs": "^1.6.0",
    "sass-loader": "^5.0.1",
    "vue-loader": "^11.1.4",
    "vue-router": "^2.4.0",
    "vue-template-compiler": "^2.2.1",
    "webpack": "^2.2.0",
    "webpack-dev-server": "^2.2.0"
  },
  "bin": "src/index.js",
  "main": "./index.js",
  "directories": {
    "bin": "./src"
  },
  "files": [
    "src/",
    "index.js"
  ],
  "keywords": [
    "vue2",
    "boostrap",
    "components",
    "vue",
    "interface",
    "layout",
    "es6",
    "front"
  ],
  "license": "MIT",
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      [
        "vueify"
      ]
    ]
  }
}
