{
  "name": "bootue",
  "description": "Bootstrap + Vue 2 = 100% Components",
  "version": "0.5.5",
  "author": "Felipe Signorini <felipeklerk@yahoo.com.br>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack-dev-server --open --hot",
    "dev-build": "cross-env NODE_ENV=development webpack --progress --hide-modules",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules",
    "lint": "eslint --ext .js,.vue src"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maestro-server/bootue.git"
  },
  "dependencies": {
    "axios": "^0.17.1",
    "izitoast": "^1.3.0",
    "maestro-vue-tables-2": "^0.1.13",
    "prismjs": "^1.9.0",
    "vue": "^2.5.11",
    "vue-notifications": "^0.9.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ],
  "devDependencies": {
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.1.2",
    "babel-loader": "^7.1.2",
    "babel-preset-env": "^1.6.0",
    "babel-preset-latest": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^4.14.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.4",
    "node-sass": "^4.5.3",
    "sass-loader": "^6.0.6",
    "url-loader": "^0.6.2",
    "vue-loader": "^13.0.5",
    "vue-router": "^3.0.1",
    "vue-template-compiler": "^2.4.4",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.9.1"
  },
  "bin": "src/index.js",
  "main": "./index.js",
  "directories": {
    "bin": "./src"
  },
  "files": [
    "src/",
    "addons/",
    "index.js"
  ],
  "keywords": [
    "vue2",
    "boostrap",
    "components",
    "vue",
    "interface",
    "layout",
    "es6",
    "front"
  ],
  "browserify": {
    "transform": [
      [
        "babelify",
        {
          "presets": [
            "es2015"
          ]
        }
      ],
      [
        "vueify"
      ]
    ]
  }
}
